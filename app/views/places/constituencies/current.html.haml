.section--primary
  .container
    %h1= title("#{@place.name} #{t('current')} #{t('constituency_plural')}")
    = render partial: 'pugin/components/navigation-letter', locals: { route_symbol: :places_constituencies_current_a_z_letter_path, route_args: [@place.graph_id] }

%section#content
  .container
    = render partial: "pugin/elements/list", locals: {item_type: "constituencies", data: @place.constituencies}
